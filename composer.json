{
  "name": "omdiaries/laravel-ai-email-assistant",
  "description": "AI-powered email generator for Laravel (9, 10, 11 compatible) supporting OpenAI and Gemini",
  "keywords": [
    "laravel",
    "email",
    "openai",
    "gemini",
    "ai",
    "email assistant",
    "generator"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Om Diaries",
      "email": "contact@omdiaries.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OmDiaries\\AIEmailAssistant\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OmDiaries\\AIEmailAssistant\\AIEmailServiceProvider"
      ],
      "aliases": {
        "AIEmail": "OmDiaries\\AIEmailAssistant\\Facades\\AIEmail"
      }
    }
  },
  "require": {
  "php": ">=8.0",
  "illuminate/support": "^9.0|^10.0|^11.0",
  "guzzlehttp/guzzle": "^7.0",
  "openai-php/client": "^0.9"
},
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
